{
  "name": "aetherspace-green-stack-starter",
  "private": true,
  "license": "MIT",
  "author": "codinsonn <thorr@codinsonn.dev>",
  "repository": "https://github.com/codinsonn/aetherspace-green-stack-starter",
  "description": "Starter monorepo to get started with the GREEN stack for all your cross platform app development needs. Comes standard with GraphQL, React-Native(-Web), Expo & Next.js + A bunch of optional helpers to make these all work together smoothlessly.",
  "workspaces": [
    "apps/*",
    "features/*",
    "packages/*"
  ],
  "devDependencies": {
    "@expo/webpack-config": "^0.16.24",
    "@react-native-community/eslint-config": "^3.0.2",
    "@storybook/addon-controls": "^6.5.5",
    "@storybook/addon-docs": "^6.5.5",
    "@storybook/addon-essentials": "^6.5.5",
    "@storybook/builder-webpack5": "^6.5.5",
    "@storybook/manager-webpack5": "^6.5.5",
    "@storybook/mdx2-csf": "^0.0.3",
    "@storybook/react": "^6.5.5",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "chromatic": "^6.5.4",
    "eslint": "^8.15.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-react-native-a11y": "^3.2.1",
    "expo-cli": "^5.4.12",
    "patch-package": "^6.4.7",
    "prettier": "^2.6.2",
    "prettier-eslint": "^14.0.3",
    "turbo": "^1.4.0",
    "webpack": "*"
  },
  "resolutions": {
    "typescript": "^4.3.5",
    "@babel/core": "^7.18.5",
    "@expo/config": "^6.0.24",
    "@types/react": "~17.0.21"
  },
  "scripts": {
    "dev": "yarn dev-mobile",
    "dev-web": "yarn turbo run next-app#dev",
    "dev-mobile": "yarn turbo run next-app#dev expo-app#start",
    "dev-docs": "yarn dev-mobile & yarn storybook",
    "next-app": "yarn workspace next-app",
    "expo-app": "yarn workspace expo-app",
    "ios": "yarn turbo run expo-app#ios",
    "android": "yarn turbo run expo-app#android",
    "expo-start": "yarn turbo run expo-app#start",
    "expo-web": "yarn turbo run expo-app#web",
    "uncache": "yarn turbo run expo-app#uncache",
    "storybook": "start-storybook -p 6006",
    "build": "yarn expo run create-next-manifest && yarn turbo run aetherspace#collect-assets build",
    "build-storybook": "build-storybook",
    "deploy": "vercel --prod --no-clipboard",
    "doppler-setup": "doppler setup",
    "doppler-sync": "doppler secrets download --no-file --format env > apps/next/.env",
    "postinstall": "patch-package"
  },
  "dependencies": {}
}
