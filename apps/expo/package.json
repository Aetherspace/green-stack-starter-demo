{
  "name": "expo-app",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "yarn expo start --web",
    "start": "yarn expo start",
    "android": "yarn expo start --android",
    "ios": "yarn expo start --ios",
    "web": "yarn expo start --web",
    "lint": "cd ../.. && eslint \"apps/expo/**/*.{ts,tsx}\"",
    "expo-cli": "yarn expo",
    "eas-cli": "npx npx eas-cli",
    "deploy:publish": "EXPO_PUBLISH=true yarn expo publish --release-channel starter-preview-main",
    "deploy:update:auto": "npx eas-cli update --auto",
    "deploy:update:dev": "npx eas-cli update --branch dev",
    "deploy:update:stage": "npx eas-cli update --branch stage",
    "deploy:update:prod": "npx eas-cli update --branch main",
    "create-next-manifest": "expo-pwa manifest --output ../next/public",
    "uncache:metro": "yarn start -c && rm -rf /tmp/metro-*"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@react-navigation/core": "^6.2.2",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/stack": "^6.1.1",
    "app": "*",
    "registries": "*",
    "expo": "^46.0.0",
    "expo-font": "~10.2.0",
    "expo-status-bar": "~1.4.0",
    "expo-updates": "^0.14.5",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.4",
    "react-native-gesture-handler": "~2.5.0",
    "react-native-reanimated": "~2.9.1",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "react-native-svg": "12.3.0",
    "react-native-web": "~0.18.7"
  },
  "devDependencies": {
    "@expo/webpack-config": "^0.17.0",
    "@types/react": "~18.0.0",
    "@types/react-native": "~0.69.1",
    "dotenv": "^10.0.0",
    "typescript": "^4.3.5"
  }
}
